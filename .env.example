APP_NAME=ParkHub
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=http://localhost:8080

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=warning

# Default admin credentials — change before first start!
# Used by the Docker entrypoint (docker-entrypoint.sh) and the browser installer (install.php)
# to create the initial admin account. Ignored if an admin already exists.
PARKHUB_ADMIN_EMAIL=admin@parkhub.local
PARKHUB_ADMIN_PASSWORD=admin

# Demo mode — set true to seed German demo data (10 lots, 200 users, ~3500 bookings) on startup.
# ONLY for demo instances. Do not enable on production.
# DEMO_MODE=false

DB_CONNECTION=sqlite
# Uncomment and configure for MySQL (Docker Compose default):
# DB_CONNECTION=mysql
# DB_HOST=db
# DB_PORT=3306
# DB_DATABASE=parkhub
# DB_USERNAME=parkhub
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail — set MAIL_MAILER=smtp for production. Use MAIL_MAILER=log during development.
# Common SMTP providers: smtp.gmail.com:587 (tls), smtp.sendgrid.net:587, smtp.mailgun.org:587
MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=smtp.example.com
MAIL_PORT=587
MAIL_USERNAME=null
MAIL_PASSWORD=null
# Note: MAIL_ENCRYPTION is a Laravel 9/10 key — Laravel 12 uses MAIL_SCHEME (set above).
MAIL_FROM_ADDRESS="noreply@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
